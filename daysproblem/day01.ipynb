   "source": [
    "from collections import Counter\n",
    "\n",
    "s = input().strip()\n",
    "\n",
    "freq = Counter(s)\n",
    "\n",
    "sorted_chars = sorted(freq.items(), key=lambda x: (-x[1], x[0]))\n",
    "\n",
    "for ch, count in sorted_chars[:3]:\n",
    "    print(ch, count)\n"
   ]
  }
